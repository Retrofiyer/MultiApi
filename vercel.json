{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "./index.html",
        "use": "@vercel/static"
      },
      {
        "src": "./Marvel/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "./StarWars/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "./Pokemon/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/marvel/(.*)",
        "dest": "/Marvel/index.js"
      },
      {
        "src": "/marvel",
        "dest": "/Marvel/index.js"
      },
      {
        "src": "/starwars/(.*)",
        "dest": "/StarWars/index.js"
      },
      {
        "src": "/starwars",
        "dest": "/StarWars/index.js"
      },
      {
        "src": "/pokemon/(.*)",
        "dest": "/Pokemon/index.js"
      },
      {
        "src": "/pokemon",
        "dest": "/Pokemon/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }  